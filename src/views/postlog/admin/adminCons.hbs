<link href="/css/sb-admin-2.min.css" rel="stylesheet">
<link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
<link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">
<link rel="stylesheet" href="css/cards.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js"></script>

<style>
    #imagen {
        width: 100%;
        height: 15vw;
        object-fit: cover;
    }
</style>

<div id="wrapper">
    <ul class="navbar-nav sidebar sidebar-dark accordion" id="accordionSidebar" style="background-color: #50aafe;">

        <!-- Sidebar - Brand -->
        <a href="/admin" class="sidebar-brand d-flex align-items-center justify-content-center text-white">
            <div>
                <h2>ADMIN</h2>
            </div>

        </a>

        <!-- Divider -->
        <hr class="sidebar-divider">

        <!-- Heading -->
        <div class="sidebar-heading">
            Interfaz
        </div>

        <!-- Nav Item - Pages Collapse Menu -->


        <!-- Nav Item - Charts -->
        <li class="nav-item">
            <a class="nav-link" href="/admin/solicitudes">
                <i class="fas fa-check-double"></i>
                <span>Solicitudes</span></a>
        </li>

        <li class="nav-item">
            <a class="nav-link" href="/admin/clientes">
                <i class="fas fa-hotel"></i>
                <span>Usuarios Clientes</span></a>
        </li>

        <li class="nav-item">
            <a class="nav-link" href="/admin/inmobiliarias">
                <i class="fas fa-pencil-ruler"></i>
                <span>Usuarios Inmobiliaria</span></a>
        </li>

        <li class="nav-item">
            <a class="nav-link" href="/admin/constructoras">
                <i class="fas fa-chart-area"></i>
                <span>Usuarios Constructora</span></a>
        </li>




        <!-- Divider -->
        <hr class="sidebar-divider d-none d-md-block">

        <!-- Sidebar Toggler (Sidebar) -->


    </ul>

    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div class="p-4" id="content">


            <!-- Begin Page Content -->
            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="d-sm-flex align-items-center justify-content-between mb-4">
                    <h1 class="h3 mb-0 text-gray-800">Clientes miembros</h1>

                </div>

                <!-- Content Row -->


                <!-- Content Row -->

                <div class="row">

                    <!-- Area Chart -->
                    <div class="col-lg">
                        <div class="card shadow mb-4">
                            <!-- Card Header - Dropdown -->
                            <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                                <h6 class="m-0 font-weight-bold text-primary">Registros</h6>

                            </div>
                            <!-- Card Body -->
                            <div class="card-body">

                                <table class="table table-fluid" id="myTable">
                                    <thead>
                                        <tr>
                                            <th>Nombre</th>
                                            <th>Correo</th>
                                            <th>Telefono</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each cons}}
                                        <tr>
                                            <td>{{fullname}}</td>
                                            <td>{{correo}}</td>
                                            <td>{{telefono}}</td>
                                            <td align="center">
                                                <a href="/deleteuser/{{idEntidad}}" class="btn btn-danger"><i class="fas fa-trash"></i></a>
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>

                            <script>
                                $(document).ready(function () {
                                    $('#myTable').DataTable({
                                        language: {
                                            "decimal": "",
                                            "emptyTable": "No hay informaci√≥n",
                                            "info": "Mostrando _START_ a _END_ de _TOTAL_ Entradas",
                                            "infoEmpty": "Mostrando 0 to 0 of 0 Entradas",
                                            "infoFiltered": "(Filtrado de _MAX_ total entradas)",
                                            "infoPostFix": "",
                                            "thousands": ",",
                                            "lengthMenu": "Mostrar _MENU_ Entradas",
                                            "loadingRecords": "Cargando...",
                                            "processing": "Procesando...",
                                            "search": "Buscar:",
                                            "zeroRecords": "Sin resultados encontrados",
                                            "paginate": {
                                                "first": "Primero",
                                                "last": "Ultimo",
                                                "next": "Siguiente",
                                                "previous": "Anterior"
                                            }
                                        },
                                        pageLength: 5,
                                        lengthMenu: [[5, 10, 20, -1], [5, 10, 20, 'Todos']]
                                    });
                                });
                            </script>


                        </div>


                    </div>
                    
                </div>
            </div>

        </div>

    </div>


</div>


</div>

<script src="/chart.min.js"></script>